generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextIndex"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model advertising {
  id          BigInt    @id @default(autoincrement())
  title       String?   @db.Text
  content     String?   @db.Text
  status      Boolean   @default(true)
  image_url   String?   @db.VarChar(255)
  default_img String?   @db.VarChar(200)
  url         String?   @db.VarChar(255)
  finish_date DateTime? @db.DateTime(0)
  deleted_at  DateTime? @db.Timestamp(0)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model cache {
  key        String @db.VarChar(255)
  value      String @db.MediumText
  expiration Int

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model cache_locks {
  key        String @db.VarChar(255)
  owner      String @db.VarChar(255)
  expiration Int

  @@ignore
}

model categories {
  id          BigInt    @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  order       Int?
  home        Boolean   @default(false)
  title       String    @db.Text
  slug        String?   @db.VarChar(255)
  sub_title   String?   @db.Text
  content     String?   @db.Text
  seo_title   String?   @db.Text
  seo_keyword String?   @db.Text
  image_url   String?   @db.VarChar(255)
  seo_desc    String?   @db.Text
  status      Boolean   @default(true)
  parent_id   String    @default("0") @db.VarChar(255)
  deleted_at  DateTime? @db.Timestamp(0)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)

  @@index([deleted_at], map: "idx_categories_deleted")
}

model category_post {
  id          BigInt    @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  category_id BigInt?
  post_id     BigInt?
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)

  @@index([category_id, post_id], map: "idx_category_post")
  @@index([category_id], map: "idx_category_post_category")
  @@index([category_id, post_id], map: "idx_category_post_lookup")
  @@index([post_id, category_id], map: "idx_category_post_postid_categoryid")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model contacts {
  id         BigInt    @db.UnsignedBigInt
  name       String    @db.Text
  email      String    @db.VarChar(255)
  phone      String    @db.VarChar(255)
  subject    String    @db.Text
  message    String    @db.Text
  deleted_at DateTime? @db.Timestamp(0)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model failed_jobs {
  id         BigInt   @db.UnsignedBigInt
  uuid       String   @db.VarChar(255)
  connection String   @db.Text
  queue      String   @db.Text
  payload    String   @db.LongText
  exception  String   @db.LongText
  failed_at  DateTime @default(now()) @db.Timestamp(0)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model menus {
  id         BigInt    @db.UnsignedBigInt
  title      String    @db.Text
  url        String?   @db.VarChar(255)
  content    String?   @db.Text
  status     Boolean   @default(false)
  parent_id  BigInt    @default(0)
  deleted_at DateTime? @db.Timestamp(0)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model messages {
  id      Int
  name    String  @db.VarChar(20)
  phone   String  @db.VarChar(25)
  email   String? @db.VarChar(50)
  date    String  @db.VarChar(256)
  doctor  String? @db.VarChar(223)
  message String  @db.Text

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model migrations {
  id        Int    @db.UnsignedInt
  migration String @db.VarChar(255)
  batch     Int

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model password_resets {
  email      String    @db.VarChar(255)
  token      String    @db.VarChar(255)
  created_at DateTime? @db.Timestamp(0)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model personal_access_tokens {
  id             BigInt    @db.UnsignedBigInt
  tokenable_type String    @db.VarChar(255)
  tokenable_id   BigInt    @db.UnsignedBigInt
  name           String    @db.VarChar(255)
  token          String    @db.VarChar(64)
  abilities      String?   @db.Text
  last_used_at   DateTime? @db.Timestamp(0)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)

  @@ignore
