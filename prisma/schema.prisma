model post_galeries {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  post_id    Int?
  name       String?   @db.VarChar(255)
  deleted_at DateTime? @db.Timestamp(0)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model posts {
  id             BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  opened_user_id Int?
  title          String    @db.Text
  title_color    Int       @default(0) @db.TinyInt
  slug           String?   @db.VarChar(500)
  keywords       String?   @db.LongText
  sub_title      String?   @db.Text
  content        String?   @db.LongText
  search         String?   @db.LongText
  seo_title      String?   @db.Text
  seo_keyword    String?   @db.Text
  seo_desc       String?   @db.Text
  view_count     BigInt    @default(0)
  count_show     Int       @default(0) @db.TinyInt
  status         Boolean   @default(true)
  publish        Int       @default(1) @db.TinyInt
  hidden         Boolean   @default(false)
  image_url      String?   @default("'iqtisadiyyat_logo_yasil-min.png'") @db.VarChar(500)
  video_url      String?   @db.VarChar(255)
  youtube_link   String?   @db.Text
  youtube_image  String?   @db.VarChar(255)
  published_date DateTime? @db.DateTime(0)
  deleted_at     DateTime? @db.Timestamp(0)
  deleted_by     Int?
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)

  @@index([deleted_at, status, published_date], map: "idx_posts_deleted_status")
  @@index([status, deleted_at, published_date(sort: Desc)], map: "idx_posts_optimized")
  @@index([status, deleted_at, published_date], map: "idx_posts_status_date")
  @@index([status, deleted_at, published_date(sort: Desc)], map: "idx_posts_status_deleted_published")
  @@index([published_date], map: "idx_published_date")
  @@index([status], map: "idx_status")
  @@index([status, published_date], map: "idx_status_published_date")
  @@fulltext([title], map: "ft_search_title")
}

model settings {
  id         BigInt    @id @default(autoincrement())
  key        String?   @db.VarChar(255)
  title      String?   @db.Text
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  deleted_at DateTime? @db.Timestamp(0)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model translates {
  id         BigInt    @db.UnsignedBigInt
  title      String    @db.Text
  key        String    @db.Text
  deleted_at DateTime? @db.Timestamp(0)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)

  @@ignore
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model users {
  id                BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name              String    @db.VarChar(255)
  surname           String?   @db.VarChar(255)
  email             String?   @unique(map: "users_email_unique") @db.VarChar(255)
  status            Boolean   @default(true)
  role              Int       @default(0) @db.TinyInt
  email_verified_at DateTime? @db.Timestamp(0)
  password          String    @db.VarChar(255)
  deleted_at        DateTime? @db.Timestamp(0)
  remember_token    String?   @db.VarChar(100)
  created_at        DateTime? @db.Timestamp(0)
  updated_at        DateTime? @db.Timestamp(0)
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model wiki_categories {
  id         BigInt       @id @default(autoincrement())
  order      Int?
  parent_id  Int          @default(0)
  title      String?      @db.LongText
  slug       String?      @db.VarChar(255)
  status     Int          @default(1) @db.TinyInt
  created_at DateTime     @default(now()) @db.Timestamp(0)
  updated_at DateTime     @default(now()) @db.Timestamp(0)
  deleted_at DateTime?    @db.Timestamp(0)
  wiki_posts wiki_posts[]
}

model wiki_posts {
  id              BigInt           @id @default(autoincrement())
  title           String?          @db.VarChar(255)
  slug            String?          @db.VarChar(255)
  sub_title       String?          @db.VarChar(255)
  content         String?          @db.Text
  image_url       String?          @db.VarChar(255)
  status          Int              @default(1) @db.TinyInt
  category_id     BigInt?
  created_at      DateTime         @default(now()) @db.Timestamp(0)
  updated_at      DateTime         @default(now()) @db.Timestamp(0)
  deleted_at      DateTime?        @db.Timestamp(0)
  wiki_categories wiki_categories? @relation(fields: [category_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "wiki_posts_ibfk_1")

  @@index([category_id], map: "category_id")
}
